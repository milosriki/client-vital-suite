# ============================================
# Backend Server - Environment Variables
# Copy to .env.local and fill in your values
# WARNING: NEVER commit .env.local to Git!
# ============================================

# Meta Conversions API Configuration
FB_PIXEL_ID=your_pixel_id_here
FB_ACCESS_TOKEN=your_access_token_here
FB_TEST_EVENT_CODE=your_test_event_code_here

# Server Configuration
PORT=3000
LOG_LEVEL=info
NODE_ENV=production

# Event Source
EVENT_SOURCE_URL=https://www.personaltrainersdubai.com

# Timezone & Currency
TZ=Asia/Dubai
DEFAULT_CURRENCY=AED

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
